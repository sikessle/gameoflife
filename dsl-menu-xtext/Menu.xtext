grammar org.sikessle.gameoflife.dsl.Menu with org.eclipse.xtext.common.Terminals

generate menu "http://www.sikessle.org/gameoflife/dsl/Menu"

Model:
	'MenuBar for' name=ID '{' menu+=Menu* '}';
    Menu:
        'menu' name=ID '{' menuItem+=MenuItem* '}';
    MenuItem:
        'menuItem' name=ID '{' 'action' action=STRING '}';
