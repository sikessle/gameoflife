grammar org.sikessle.gameoflife.dsl.Figure with org.eclipse.xtext.common.Terminals

generate figure "http://www.sikessle.org/gameoflife/dsl/Figure"

Model:
	figures += Figure*
;
	
Figure:
	'Figure' name=ID '{'
		rows += Row+
	'}'
;

Row:
	':' (cells += Cell+)
;

Cell:
	(alive ?= '#') | (dead ?= '-')
;